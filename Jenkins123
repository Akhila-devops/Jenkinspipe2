pipeline {
  agent {
  label "new node"
  }
stages {
    stage ('checkout') {
       steps {
        checkout scm
        }
    }
    stage ('creation of the folder') {
       steps {
          sh 'cd /home/ubuntu; sudo mkdir rahul1'
       }
    }
    stage ('creation of the folder on different server') {
      steps {
        label ('slave')
        sh 'cd /home/ubuntu; sudo mkdir rohith1'
        }
    }
}
}
