pipeline {
agent any
stages {
    stage ('checkout') {
       steps {
          checkout scm
        }
    }
    stage ('creation of the folder') {
       steps {
       node ('new node')
         sh 'cd /home/ubuntu; sudo mkdir rahul'
       }
    }
    stage ('creation of the folder on different server') {
      steps {
        node ('node demo')
        sh 'cd /var/www; sudo mkdir rohith'
        }
    }
}
}
