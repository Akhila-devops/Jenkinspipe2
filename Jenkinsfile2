pipeline {
agent any
label 'new node'
}

stages {
  stage ('checkout')
  {
   steps
       {
        
         checkout scm

       }
  }

  stage ('creation of the folder')
  {
      steps
      {
        sh 'cd /home/ubuntu; sudo mkdir rahul'
      }
  }
  
  stage ('creation of the folder on different server')
  {
      steps
      {
        node ('node demo')
        {
         sh 'cd /home/ubuntu; sudo mkdir rohith'
        }
      }  

   }
}
